{
  "$schema": "D:/projects/hawaii-cli/todo-005.json",
  "data-source": {
    "database-type": "mssql",
    "resolver-config-file": "",
    "connection-string": ""
  },
  "runtime": {
    "Rest": {
      "Enabled": true,
      "Path": "/api"
    },
    "GraphQL": {
      "allow-introspection": true,
      "Enabled": true,
      "Path": "/api/graphql"
    },
    "Host": {
      "Mode": "Production"
    }
  },
  "entities": {
    "todo": {
      "source": "s005.todos",
      "permissions": [
        {
          "role": "authenticated",
          "actions": [
            "*"
          ]
        }
      ],
      "relationships": {
        "category": {
          "cardinality": "One",
          "target.entity": "category",
          "source.fields": [
            "category_id"
          ],
          "target.fields": [
            "id"
          ]
        },
        "owner": {
          "cardinality": "One",
          "target.entity": "user",
          "source.fields": [
            "owner_id"
          ],
          "target.fields": [
            "id"
          ]
        }
      }
    },
    "user": {
      "source": "s005.users",
      "permissions": [
        {
          "role": "authenticated",
          "actions": [
            "*"
          ]
        }
      ],
      "relationships": {
        "owns": {
          "cardinality": "Many",
          "target.entity": "todo",
          "source.fields": [
            "id"
          ],
          "target.fields": [
            "owner_id"
          ]
        }
      }
    },
    "category": {
      "source": "s005.categories",
      "graphQL": {
        "type": {
          "singular": "category",
          "plural": "categories"
        }
      },
      "permissions": [
        {
          "role": "authenticated",
          "actions": [
            "read"
          ]
        }
      ],
      "relationships": {
        "todos": {
          "cardinality": "Many",
          "target.entity": "todo",
          "source.fields": [
            "id"
          ],
          "target.fields": [
            "category_id"
          ]
        }
      }
    }
  }
}