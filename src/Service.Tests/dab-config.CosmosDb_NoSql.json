        {
          "role": "authenticated",
          "actions": [
            "create",
            "read",
            "update",
            "delete"
          ]
        }
      ],
      "graphql": {
        "type": {
          "singular": "Star",
          "plural": "Stars"
        }
      }
    },
    "Moon": {
      "source": "graphqldb.moon",
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "create",
            "read",
            "update",
            "delete"
          ]
        },
        {
          "role": "authenticated",
          "actions": [
            "create",
